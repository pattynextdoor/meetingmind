{
  "name": "meetingmind",
  "version": "1.0.1",
  "description": "Automatically import meeting transcripts into your vault with AI-powered enrichment and intelligent auto-linking",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit --project tsconfig.json && node esbuild.config.mjs production",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "meeting",
    "transcript",
    "ai"
  ],
  "author": "Patrick",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.8",
    "eslint": "^9.39.2",
    "obsidian": "latest",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.50.0"
  }
}
